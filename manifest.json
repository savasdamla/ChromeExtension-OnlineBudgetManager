{
    "manifest_version": 3,
    "name": "Budget Manager",
    "version": "1.0",
    "description": "Second extension",
    "icons": {
        "16": "/images/b_icon-16.png",
        "48": "/images/b_icon-48.png",
        "128": "/images/b_icon-128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "/images/b_icon-16.png"
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "eventPage.js"
    },
    "permissions": [
        "storage",
        "notifications",
        "contextMenus"
    ]
}